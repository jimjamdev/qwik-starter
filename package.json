{
  "name": "my-qwik-basic-starter",
  "description": "Recommended for your first Qwik app (comes with Qwik City)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint ./src/**/*.tsx",
    "lint:fix": "eslint ./src/**/*.tsx --fix",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test": "npm run lint:fix",
    "qwik": "qwik"
  },
  "overrides": {
    "vite": "$vite"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.5",
    "@builder.io/qwik": "^0.20.1",
    "@builder.io/qwik-city": "^0.5.2",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vanilla-extract/css": "^1.9.5",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-qwik": "^0.20.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "styled-vanilla-extract": "^0.5.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vercel": "^28.16.12",
    "vite": "^4.1.4",
    "vite-imagetools": "^4.0.18",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "@auth/core": "^0.5.1",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "clsx": "^1.2.1",
    "framer-motion": "^10.1.0",
    "motion": "^10.15.5",
    "rainbow-sprinkles": "^0.16.0",
    "swiper": "^9.1.0",
    "undici": "^5.20.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.6"
  }
}
